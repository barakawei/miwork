  <process id="purchasing-flow" name="面辅料采购流程" isExecutable="true">
    <startEvent id="start" name="开始" activiti:initiator="planning" activiti:formKey="planning"></startEvent>
    <userTask id="purchasing" name="采购" activiti:candidateGroups="role_purchasing">
      <extensionElements>
        <activiti:taskListener event="complete" delegateExpression="${purchasingProcessor}"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <userTask id="warehouseEntryPlan" name="预入库" activiti:candidateGroups="role_warehouse">
      <extensionElements>
        <activiti:taskListener event="complete" delegateExpression="${warehouseEntryPlanProcessor}"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <userTask id="quality" name="检测" activiti:candidateGroups="role_quality">
      <extensionElements>
        <activiti:taskListener event="complete" delegateExpression="${qualityProcessor}"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <exclusiveGateway id="eg1"></exclusiveGateway>
    <endEvent id="end" name="结束"></endEvent>
    <userTask id="tech" name="技术" activiti:candidateGroups="role_technolog">
      <extensionElements>
        <activiti:taskListener event="complete" delegateExpression="${techProcessor}"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <userTask id="leaderAudit" name="领导审批" activiti:candidateGroups="role_leader">
      <extensionElements>
        <activiti:taskListener event="complete" delegateExpression="${leaderAuditProcessor}"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <exclusiveGateway id="eg2"></exclusiveGateway>
    <userTask id="warehouseEntryActual" name="入库" activiti:candidateGroups="role_warehouse">
      <extensionElements>
        <activiti:taskListener event="complete" delegateExpression="${warehouseEntryActualProcessor}"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow1" sourceRef="start" targetRef="purchasing"></sequenceFlow>
    <sequenceFlow id="flow3" sourceRef="purchasing" targetRef="warehouseEntryPlan"></sequenceFlow>
    <sequenceFlow id="flow4" sourceRef="warehouseEntryPlan" targetRef="quality"></sequenceFlow>
    <sequenceFlow id="flow5" sourceRef="quality" targetRef="eg1"></sequenceFlow>
    <sequenceFlow id="flow6" name="不合格" sourceRef="eg1" targetRef="leaderAudit">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${check==false}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow7" name="审批" sourceRef="leaderAudit" targetRef="eg2"></sequenceFlow>
    <sequenceFlow id="flow13" name="采购辅料" sourceRef="tech" targetRef="purchasing"></sequenceFlow>
    <sequenceFlow id="flow12" name="合格" sourceRef="eg1" targetRef="tech">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${check==true && other == true}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow14" sourceRef="warehouseEntryActual" targetRef="end"></sequenceFlow>
    <sequenceFlow id="flow9" name="特批通过" sourceRef="eg2" targetRef="tech">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${comment==true && other==true}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow10" sourceRef="eg2" targetRef="warehouseEntryActual">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${comment==true && other==false}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow8" name="重新采购" sourceRef="eg2" targetRef="purchasing">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${comment==false}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow11" name="入库" sourceRef="eg1" targetRef="warehouseEntryActual">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${check==true && other==false}]]></conditionExpression>
    </sequenceFlow>
  </process>
